{
    "name": "goldfinch/video-field",
    "description": "YouTube & Vimeo video field for Silverstripe. Store video data for further manipulation. Enchant links with parameters through friendly interface, display thumbnails, fetching video data like title, description and more with no extra actions",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "video-field",
        "videos-field",
        "silverstripe-video"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Art Karasev",
            "email": "art.karasev@proton.me"
        }
    ],
    "require": {
        "php": ">=8.0",
        "silverstripe/framework": "^5.0",
        "silverstripe/admin": "^2.0",
        "goldfinch/taz": "^2.0",
        "goldfinch/silverstripe-jsontext": "^2.1"
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
    },
    "autoload": {
        "psr-4": {
            "Goldfinch\\VideoField\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
